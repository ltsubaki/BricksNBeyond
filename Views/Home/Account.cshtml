@{
}
@model List<Customers>
@foreach (Customers c in Model)
{
    <div class="text-center">
        <ul>
            <li>First: @c.FirstName</li>
            <li>Last: @c.LastName</li>
            <li>Birthday: @c.BirthDate</li>
            <li>Residential Country: @c.ResCountry</li>
            <li>Gender: @c.Gender</li>
            <li>Age: @c.Age</li>

        </ul>
    </div>
}
